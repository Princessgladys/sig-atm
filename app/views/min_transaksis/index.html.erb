<div class="span5">
  <table class="table table-striped table-bordered">
    <tr>
      <th>Nominal (Rp.)</th>
      <th>Pilihan</th>
    </tr>

  <% @min_transaksis.each do |min_transaksi| %>
    <tr>
      <td><%= min_transaksi.nominal %></td>
      <td>
        <%= link_to 'Edit', edit_min_transaksi_path(min_transaksi) %> |
        <%= link_to 'Delete', min_transaksi, method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
  </table>

  <%= link_to '+ Tambah', new_min_transaksi_path, class: "btn btn-primary" %>
</div>